{
  "name": "jerrytfleung/opentelemetry",
  "type": "metapackage",
  "description": "OpenTelemetry metapackage",
  "keywords": ["swo", "instrumentation", "performance", "opentelemetry", "observability"],
  "readme": "./README.md",
  "license": "Apache-2.0",
  "minimum-stability": "stable",
  "require": {
    "open-telemetry/api": "@stable",
    "open-telemetry/context": "@stable",
    "open-telemetry/exporter-otlp": "@stable",
    "open-telemetry/opentelemetry-auto-aws-sdk": "@stable",
    "open-telemetry/opentelemetry-auto-cakephp": "@stable",
    "open-telemetry/opentelemetry-auto-codeigniter": "@stable",
    "open-telemetry/opentelemetry-auto-curl": "@stable",
    "open-telemetry/opentelemetry-auto-doctrine": "@stable",
    "open-telemetry/opentelemetry-auto-ext-amqp": "@stable",
    "open-telemetry/opentelemetry-auto-ext-rdkafka": "@stable",
    "open-telemetry/opentelemetry-auto-guzzle": "@stable",
    "open-telemetry/opentelemetry-auto-http-async": "@stable",
    "open-telemetry/opentelemetry-auto-io": "@stable",
    "open-telemetry/opentelemetry-auto-laravel": "@stable",
    "open-telemetry/opentelemetry-auto-mongodb": "@stable",
    "open-telemetry/opentelemetry-auto-mysqli": "@stable",
    "open-telemetry/opentelemetry-auto-openai-php": "@stable",
    "open-telemetry/opentelemetry-auto-pdo": "@stable",
    "open-telemetry/opentelemetry-auto-psr3": "@stable",
    "open-telemetry/opentelemetry-auto-psr6": "@stable",
    "open-telemetry/opentelemetry-auto-psr14": "@stable",
    "open-telemetry/opentelemetry-auto-psr15": "@stable",
    "open-telemetry/opentelemetry-auto-psr16": "@stable",
    "open-telemetry/opentelemetry-auto-psr18": "@stable",
    "open-telemetry/opentelemetry-auto-reactphp": "@stable",
    "open-telemetry/opentelemetry-auto-slim": "@stable",
    "open-telemetry/opentelemetry-auto-symfony": "@stable",
    "open-telemetry/opentelemetry-auto-wordpress": "@stable",
    "open-telemetry/opentelemetry-auto-yii": "@stable",
    "solarwinds/apm": "@stable",
    "symfony/http-client": "^5|^6",
    "nyholm/psr7": "^1.5"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": false
    }
  }
}
