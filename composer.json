{
  "name": "jerrytfleung/opentelemetry",
  "type": "metapackage",
  "description": "OpenTelemetry metapackage",
  "keywords": ["swo", "instrumentation", "performance", "opentelemetry", "observability"],
  "readme": "./README.md",
  "license": "Apache-2.0",
  "minimum-stability": "stable",
  "require": {
    "open-telemetry/api": "@beta",
    "open-telemetry/context": "@beta",
    "open-telemetry/exporter-otlp": "@beta",
    "open-telemetry/opentelemetry-auto-aws-sdk": "@beta",
    "open-telemetry/opentelemetry-auto-cakephp": "@beta",
    "open-telemetry/opentelemetry-auto-codeigniter": "@beta",
    "open-telemetry/opentelemetry-auto-curl": "@beta",
    "open-telemetry/opentelemetry-auto-doctrine": "@beta",
    "open-telemetry/opentelemetry-auto-ext-amqp": "@beta",
    "open-telemetry/opentelemetry-auto-ext-rdkafka": "@beta",
    "open-telemetry/opentelemetry-auto-guzzle": "@beta",
    "open-telemetry/opentelemetry-auto-http-async": "@beta",
    "open-telemetry/opentelemetry-auto-io": "@beta",
    "open-telemetry/opentelemetry-auto-laravel": "@beta",
    "open-telemetry/opentelemetry-auto-mongodb": "@beta",
    "open-telemetry/opentelemetry-auto-mysqli": "@beta",
    "open-telemetry/opentelemetry-auto-openai-php": "@beta",
    "open-telemetry/opentelemetry-auto-pdo": "@beta",
    "open-telemetry/opentelemetry-auto-psr3": "@beta",
    "open-telemetry/opentelemetry-auto-psr6": "@beta",
    "open-telemetry/opentelemetry-auto-psr14": "@beta",
    "open-telemetry/opentelemetry-auto-psr15": "@beta",
    "open-telemetry/opentelemetry-auto-psr16": "@beta",
    "open-telemetry/opentelemetry-auto-psr18": "@beta",
    "open-telemetry/opentelemetry-auto-reactphp": "@beta",
    "open-telemetry/opentelemetry-auto-slim": "@beta",
    "open-telemetry/opentelemetry-auto-symfony": "@beta",
    "open-telemetry/opentelemetry-auto-wordpress": "@beta",
    "open-telemetry/opentelemetry-auto-yii": "@beta",
    "open-telemetry/opentelemetry-logger-monolog": "@beta",
    "solarwinds/apm": "@beta",
    "symfony/http-client": "^5|^6",
    "nyholm/psr7": "^1.5"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": false
    }
  }
}
